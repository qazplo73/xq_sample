if close > value1 then condition1 =true;       //condition1 = close > value1;
if value1 > value2 then condition2 =true;      //condition2 = value1 > value2;
print(condition1,condition2);
if condition1 and condition2 then ret = 1;
  


if condition3 and condition4 then begin  //如果條件三與條件四吻合就開始執行第九行
    if value10 > value20 then ret = 1; 
end                                      //執行第九行後,跳出此迴圈
  else                                   //如果條件三與條件四其中一項不吻合吻合就開始執行第12~18行
    if condition5 and condition6 then    //如果條件五與條件六吻合就開始執行第14行
      begin
        if value10 > value20 then ret = 1; 
        end
    else                                 //如果條件五與條件六其中一項不吻合就開始執行第1行
      begin
        if condition4 and condition5 then ret = 1;
end;
